{
  "manifest_version": 2,

  "name": "React Perf Tools",
  "description": "Perf prototype",
  "version": "1.0",
  "devtools_page": "devtools.html",

  "icons": {
    "16": "./React-Perf.png",
    "48": "./React-Perf.png",
    "128": "./React-Perf.png"
  },

  "permissions": [
    "file:///*",
    "http://*/*",
    "https://*/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
